<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="8.0.6"/>
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="8.0.6" PrivateAssets="all"/>
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="wwwroot\js\waveform\waveform-data-channel.js" />
      <_ContentIncludedByDefault Remove="wwwroot\js\waveform\waveform-data-worker.js" />
      <_ContentIncludedByDefault Remove="wwwroot\js\waveform\waveform-data.js" />
      <_ContentIncludedByDefault Remove="wwwroot\js\waveform\waveform-generator.js" />
      <_ContentIncludedByDefault Remove="wwwroot\js\waveform\waveform-utils.js" />
    </ItemGroup>

</Project>
